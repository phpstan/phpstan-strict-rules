			universalObjectCratesClasses: %universalObjectCratesClasses%

	-
		class: PHPStan\Rules\DisallowedConstructs\DisallowedLooseComparisonRule

	-
		class: PHPStan\Rules\BooleansInConditions\BooleanInBooleanAndRule
