	-
		class: PHPStan\Rules\BooleansInConditions\BooleanRuleHelper

	-
		class: PHPStan\Rules\Operators\OperatorRuleHelper

	-
		class: PHPStan\Rules\VariableVariables\VariablePropertyFetchRule
